{
  "mjolnir": {
    "id_table_size": 1300000000,
    "tile_dir": "/custom_files/valhalla_tiles",
    "admin": "/custom_files/valhalla_tiles/admins.sqlite",
    "timezone": "/custom_files/valhalla_tiles/timezones.sqlite",
    "transit_dir": "/custom_files/valhalla_tiles/transit"
  },
  "additional_data": {
    "elevation": "/custom_files/valhalla_tiles/elevation/"
  },
  "loki": {
    "actions": ["locate", "route", "height", "optimized_route", "isochrone", "trace_route", "trace_attributes", "transit_available", "expansion", "centroid", "status"],
    "logging": {
      "long_request": 200.0
    },
    "service_defaults": {
      "minimum_reachability": 50,
      "radius": 0,
      "search_cutoff": 35000,
      "node_snap_tolerance": 5,
      "street_side_tolerance": 5,
      "street_side_max_distance": 1000,
      "heading_tolerance": 60
    },
    "service": {
      "proxy": "ipc:///tmp/loki"
    }
  },
  "thor": {
    "logging": {
      "long_request": 200.0
    },
    "source_to_target_algorithm": "select_optimal",
    "service": {
      "proxy": "ipc:///tmp/thor"
    }
  },
  "odin": {
    "logging": {
      "long_request": 200.0
    },
    "service": {
      "proxy": "ipc:///tmp/odin"
    }
  },
  "skadi": {
    "actons": ["height"],
    "logging": {
      "long_request": 5.0
    },
    "service": {
      "proxy": "ipc:///tmp/skadi"
    }
  },
  "meili": {
    "customizable": ["turn_penalty_factor", "max_route_distance_factor", "max_route_time_factor"],
    "mode": "auto",
    "grid": {
      "cache_size": 100240,
      "size": 500
    },
    "default": {
      "beta": 3,
      "breakage_distance": 2000,
      "geometry": false,
      "gps_accuracy": 5.0,
      "interpolation_distance": 10,
      "max_route_distance_factor": 5,
      "max_route_time_factor": 5,
      "max_search_radius": 200,
      "route": true,
      "search_radius": 15.0,
      "sigma_z": 4.07,
      "turn_penalty_factor": 200
    },
    "auto": {
      "turn_penalty_factor": 200,
      "search_radius": 15.0
    },
    "pedestrian": {
      "turn_penalty_factor": 100,
      "search_radius": 25.0
    },
    "bicycle": {
      "turn_penalty_factor": 140
    },
    "bus": {
      "turn_penalty_factor": 180
    },
    "service": {
      "proxy": "ipc:///tmp/meili"
    }
  },
  "httpd": {
    "service": {
      "listen": "tcp://*:8002",
      "loopback": "ipc:///tmp/loopback",
      "interrupt": "ipc:///tmp/interrupt"
    }
  },
  "service_limits": {
    "auto": {
      "max_distance": 5000000.0,
